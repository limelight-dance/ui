{
    "env": { "browser": true },
    "extends": "airbnb",
    "parserOptions": { "ecmaVersion": 2018 },
    "rules": {
        "semi": [2, "never"],
        "no-console": 0,
        "import/extensions": [2, "ignorePackages"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/no-danger": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-a11y/click-events-have-key-events": 0
    }
}